  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "def rev(str):\n",
    "    temp = []\n",
    "    for i in range(len(str)-1,-1,-1):\n",
    "        temp.append(str[i])\n",
    "    return \"\".join(temp)"
   ]
